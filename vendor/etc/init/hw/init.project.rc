 service vendor_tcpdump /vendor/bin/tcpdump-vendor -i any -W 3 -C 5 -s 134 -w /data/vendor/wlan_logs/tcpdump.pcap
    class main
    user root
    group root wifi
    disabled
    oneshot

#on property:sys.user.0.ce_available=true
#    start vendor_tcpdump

on property:ro.debuggable=1
    setprop persist.vendor.tcpdump.enable true
    setprop persist.vendor.connsysfw.enable true

# wifi-add: start/stop wlan logs service
on property:persist.vendor.tcpdump.enable=true
    start vendor_tcpdump

on property:persist.vendor.tcpdump.enable=false
    stop vendor_tcpdump

# Xiaomi, DSIM, dynamic MSIM Switch depends on ro.boot.multisim
on property:persist.vendor.radio.msimmode=ss
   setprop ro.vendor.miui.singlesim 1
   setprop ro.miui.singlesim 1
   setprop ro.telephony.sim.count 1
   setprop telephony.active_modems.max_count 1

on property:persist.vendor.radio.msimmode=dsds
   setprop ro.vendor.miui.singlesim 0
   setprop ro.miui.singlesim 0
   setprop ro.telephony.sim.count 2
   setprop telephony.active_modems.max_count 2

on property:persist.sys.cota.carrier=OR
    stop vendor_tcpdump

on property:persist.vendor.connsysfw.enable=true
    start connsyslogger

on property:persist.vendor.connsysfw.enable=false && property:vendor.connsysfw.running=0
    stop connsyslogger

service vendor_sniffer /vendor/bin/tcpdump-vendor -i wlan0 -w /data/vendor/wlan_logs/sniffer.pcap
    class main
    user root
    group root wifi
    disabled
    oneshot

on property:vendor.hardware.wlan.runsniffer=stop
    stop vendor_sniffer

on property:vendor.hardware.wlan.runsniffer=start
    start vendor_sniffer

on property:vendor.hardware.wlan.runtcpdump=stop
    stop vendor_tcpdump

on property:vendor.hardware.wlan.runtcpdump=start
    start vendor_tcpdump

service vendor_sniffer_circular /vendor/bin/tcpdump-vendor -i wlan0 -W 3 -C 300 -w /data/vendor/wlan_logs/sniffer.pcap
    class main
    user root
    group root wifi
    disabled
    oneshot

on property:vendor.hardware.wlan.runsniffer.circular=stop
     stop vendor_sniffer_circular

on property:vendor.hardware.wlan.runsniffer.circular=start
     start vendor_sniffer_circular

service vendor_sniffer_init /vendor/bin/wifi_sniffer_init
    class main
    user root
    group root wifi
    disabled
    oneshot

on property:vendor.hardware.wlan.runsniffer.cmd=stop
     stop vendor_sniffer_init

on property:vendor.hardware.wlan.runsniffer.cmd=start
     start vendor_sniffer_init
